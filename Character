package Game;

public interface Character {
	
	String getName();
	int getHealth();
	int getAttackPower();
	int getHeal();
	int getExperience();
	void setAttackPower(int attackPower);
	void gainExperience();
	
	
	
	
	int decreaseHealth(int opponentAttackPower);
	void attack(Character opponent);
	void status(Character character);
	boolean isDead();
	void jump();
	void run();
	void say();
	int healUp();
	

	
	
	

}
